{
  "name": "ffxiv-lib",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "files": [
    "assets",
