{
  "name": "ffxiv-lib",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/ffxiv-lib.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write",
    "build:resources": "pushd util & powershell -NoProfile -ExecutionPolicy Unrestricted -Command .\\build.ps1 & popd & prettier --write ./src/resources/*.ts ./src/assets/icons/*.htm",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "dts-bundle-generator": "^9.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "extract-zip": "^2.0.1",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
