{
  "name": "ffxiv-lib",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write",
    "build:resources": "ts-node --project ./util/tsconfig.json --esm ./util/generate.ts",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "dts-bundle-generator": "^9.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "extract-zip": "^2.0.1",
    "fast-csv": "^4.3.6",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
