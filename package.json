    "@vitest/coverage-v8": "^1.5.3",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "extract-zip": "^2.0.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.3"
